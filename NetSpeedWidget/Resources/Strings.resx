<?xml version="1.0" encoding="utf-8"?>
<root>
  <xsd:schema id="root" xmlns="" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
    <xsd:element name="root" msdata:IsDataSet="true">
      <xsd:complexType>
        <xsd:choice maxOccurs="unbounded">
          <xsd:element name="data">
            <xsd:complexType>
              <xsd:sequence>
                <xsd:element name="value" type="xsd:string" minOccurs="0" msdata:Ordinal="1" />
              </xsd:sequence>
              <xsd:attribute name="name" type="xsd:string" use="required" />
              <xsd:attribute name="type" type="xsd:string" />
            </xsd:complexType>
          </xsd:element>
        </xsd:choice>
      </xsd:complexType>
    </xsd:element>
  </xsd:schema>
  
  <!-- App Title -->
  <data name="AppTitle" xml:space="preserve">
    <value>Network Speed Widget</value>
  </data>
  <data name="NetworkUsage" xml:space="preserve">
    <value>Network Usage</value>
  </data>

  <!-- Settings Window -->
  <data name="Settings" xml:space="preserve">
    <value>Settings</value>
  </data>
  <data name="StartWithWindows" xml:space="preserve">
    <value>Start with Windows</value>
  </data>
  <data name="Language" xml:space="preserve">
    <value>Language</value>
  </data>

  <!-- System Tray -->
  <data name="Show" xml:space="preserve">
    <value>Show</value>
  </data>
  <data name="Exit" xml:space="preserve">
    <value>Exit</value>
  </data>

  <!-- Messages -->
  <data name="LanguageChanged" xml:space="preserve">
    <value>Language Changed</value>
  </data>
  <data name="RestartRequired" xml:space="preserve">
    <value>Language setting will be fully applied after restarting the application.</value>
  </data>
  <data name="Error" xml:space="preserve">
    <value>Error</value>
  </data>
  <data name="RegistryAccessError" xml:space="preserve">
    <value>Failed to access registry.</value>
  </data>
  <data name="StartupError" xml:space="preserve">
    <value>Failed to update startup settings: {0}</value>
  </data>
  <data name="LanguageError" xml:space="preserve">
    <value>Failed to apply language: {0}</value>
  </data>
  <data name="SaveLanguageError" xml:space="preserve">
    <value>Failed to save language setting: {0}</value>
  </data>
  <data name="TotalDownload" xml:space="preserve">
    <value>Total Download: </value>
  </data>
  <data name="TotalUpload" xml:space="preserve">
    <value>Total Upload: </value>
  </data>
  <data name="DownloadSpeed" xml:space="preserve">
    <value>Download Speed (MB/s)</value>
  </data>
  <data name="UploadSpeed" xml:space="preserve">
    <value>Upload Speed (MB/s)</value>
  </data>
  <data name="ApplicationUsage" xml:space="preserve">
    <value>Application Usage</value>
  </data>
</root> 